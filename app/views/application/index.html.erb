<div class='row-fluid'>
  <div class='span12'>
    <h1>Cors Tester Application</h1> 
  </div>
</div>
<div class='row-fluid'>
  <div class='span6 offset3'>
    <form id='send-it'>
      <input type="text"    id="first_name"      name="first_name" placeholder="First Name*" class="name">
      <input type="text"    id="last_name"       name="last_name"  placeholder="Last Name*" class="name">
      <input type="text"    id="email"           name="email"      placeholder="Email*" class="name">
      <input type="text"    id="phone"           name="phone"      placeholder="Phone*" class="name">
      <input type="hidden"  id="content-url"     name="content_url" value="stock">
      <input type="submit"  id="send-the-form"   name="submit" value="Submit" class="btn btn-primary btn-large">
    </form>
  </div>
</div>
